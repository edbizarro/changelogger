{
    "name": "edbizarro/changelogger",
    "description": "",
    "keywords": ["changelog", "laravel", "laravel-zero"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/edbizarro/changelogger/issues",
        "source": "https://github.com/edbizarro/changelogger"
    },
    "authors": [
        {
            "name": "Eduardo Bizarro",
            "email": "edbizarro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.13",
        "erusev/parsedown": "^1.7",
        "illuminate/filesystem": "5.6.*",
        "laravel-zero/framework": "5.6.*",
        "symfony/yaml": "^3.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": ["changelogger"]
}
